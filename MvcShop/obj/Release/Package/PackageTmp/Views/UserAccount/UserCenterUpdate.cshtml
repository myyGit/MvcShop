
@{
    ViewBag.Title = "个人信息修改";
    Layout = "~/Views/Shared/_LayoutOne.cshtml";
}
<link href="~/page/user-center-update/index.css" rel="stylesheet" />
<script src="~/page/user-center-update/index.js"></script>

<div class="crumb">
    <div class="w">
        <div class="crumb-con">
            <a class="link" href="./index.html">MMall</a>
            <span>></span>
            <span class="link-text">个人信息修改</span>
        </div>
    </div>
</div>
<div class="page-wrap w">
    <div id="navSideDiv"></div>
    <div class="content with-nav">
        <div class="panel">
            <div class="panel-title">个人中心</div>
            <div class="user-info">
                <div class="form-line">
                    <span class="label">用户名：</span>
                    <span class="text">张三</span>
                </div>
                <div class="form-line">
                    <span class="label">电 话：</span>
                    <input class="input" id="phone" autocomplete="off" value="13300000000" />
                </div>
                <div class="form-line">
                    <span class="label">邮 箱：</span>
                    <input class="input" id="email" autocomplete="off" value="12345678@qq.com" />
                </div>
                <div class="form-line">
                    <span class="label">问 题：</span>
                    <input class="input" id="question" autocomplete="off" value="我的名字" />
                </div>
                <div class="form-line">
                    <span class="label">答 案：</span>
                    <input class="input" id="answer" autocomplete="off" value="你猜呐" />
                </div>
                <span class="btn btn-submit">提交</span>
            </div>
            <!--<div class="panel-body">
                <div class="loading"></div>
            </div>-->
        </div>
    </div>
</div>


